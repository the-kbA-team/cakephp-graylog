{
    "name": "kba-team/cakephp-graylog",
    "description": "Graylog engine for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "cakephp/cakephp": "~5.0",
        "graylog2/gelf-php": "^2.0.1",
        "kba-team/php-backtrace": "^1.0",
        "kba-team/graylog-utilities": "^2.0.1"
    },
    "extra": {
        "installer-name": "Graylog"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "kbATeam\\CakePhpGraylog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\kbATeam\\CakePhpGraylog\\": "tests/"
        }
    }
}
